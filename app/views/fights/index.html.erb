<h1 class='align-center'>Who Would Win?</h1>

<div class="star-wars">
  <%= image_tag @star_war.image %>
  <h3><%= @star_war.name %></h3>
  <%= button_to @star_war.name, update_starwars_path(@star_war), method: :patch%>
<div class="pokemon">
  <%= image_tag @pokemon.image %>
  <h3><%= @pokemon.name %></h3>
  <%= button_to @pokemon.name, update_pokemon_path(@pokemon), method: :patch%>
</div>